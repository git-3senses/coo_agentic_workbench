# ─── Database (Railway MySQL provides these via ${{MySQL.*}}) ───
DB_HOST=localhost
DB_PORT=3306
DB_USER=npa_user
DB_PASSWORD=npa_password
DB_NAME=npa_workbench

# ─── Server Ports ───
MCP_PORT=3001
REST_PORT=3002

# ─── Public URL (set to your Railway domain) ───
# Used in OpenAPI spec so Dify knows where to call tools
PUBLIC_URL=http://localhost:3002

# ─── Environment ───
ENV=development
